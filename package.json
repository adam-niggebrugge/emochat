{
  "name": "emochat",
  "version": "1.0.0",
  "description": "A fun way to communicate without text",
  "main": "index.js",
  "engines": {
    "node": "14.x",
    "npm": "7.x"
  },
  "scripts": {
    "start": "node server/server.js",
    "develop": "concurrently \"cd server && npm run watch\" \"cd client && npm start\"",
    "install": "cd server && npm i && cd ../client && npm i",
    "seed": "cd server && npm run seed",
    "build": "cd client && npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/adam-niggebrugge/emochat.git"
  },
  "keywords": [
    "socketIO",
    "express"
  ],
  "author": "Jeff Cela, Sebastian Campos, Hudson Hancock, Adam Niggebrugge",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/adam-niggebrugge/emochat/issues"
  },
  "homepage": "https://github.com/adam-niggebrugge/emochat#readme",
  "dependencies": {
    "@heroku/buildpack-registry": "^1.0.1",
    "concurrently": "^6.4.0"
  }
}
