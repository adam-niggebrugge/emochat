{
  "name": "emochat",
  "version": "1.0.0",
  "description": "A fun way to communicate without text",
  "main": "index.js",
  "engines": {
    "node": "14.x",
    "npm": "7.x"
  },
  "scripts": {
    "start": "node server/server.js",
    "develop": "concurrently \"cd server && npm run watch\" \"cd client && npm start\"",
    "install": "cd server && npm i && cd ../client && npm i",
    "seed": "cd server && npm run seed",
    "build": "cd client && npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/adam-niggebrugge/emochat.git"
  },
  "keywords": [
    "socketIO",
    "express"
  ],
  "author": "Jeff Cela, Sebastian Campos, Hudson Hancock, Adam Niggebrugge",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/adam-niggebrugge/emochat/issues"
  },
  "homepage": "https://github.com/adam-niggebrugge/emochat#readme",
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "bootstrap-icons": "^1.7.1",
    "colors": "^1.4.0",
    "concurrently": "^6.4.0",
    "dotenv": "^9.0.2",
    "express": "^4.17.1",
    "express-async-handler": "^1.1.4",
    "jsonwebtoken": "^8.5.1",
    "mongoose": "^5.12.9",
    "nodemon": "^2.0.7",
    "react-router-dom": "^6.0.2",
    "socket.io": "^4.1.2"
  },
  "optionalDependencies": {
    "fsevents": "^2.3.2"
  }
}
